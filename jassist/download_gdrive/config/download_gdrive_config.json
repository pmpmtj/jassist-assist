{
    "file_types": {
        "include": [".mp3", ".wav", ".m4a", ".ogg", ".flac", ".aac", ".pdf", ".docx", ".xlsx"],
        "exclude": []
    },
    "folders": {
        "target_folders": ["root"]
    },
    "download": {
        "add_timestamps": true,
        "timestamp_format": "%Y%m%d_%H%M%S_%f",
        "delete_after_download": true,
        "dry_run": false
    }
}